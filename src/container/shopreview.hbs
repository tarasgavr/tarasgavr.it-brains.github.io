{{#with navigation}}  
  <section class="mb-3">
    <div class="row card">
      <ul class="nav">
          {{#each links}}
        <li class="nav-item">
          <a class="nav-link {{#if @last}} disabled {{/if}}" href="{{href}}">{{text}}</a>
        </li>
          {{/each}}
      </ul>
    </div>
  </section>
{{/with}}
<div class="row">
  <div class="col-8">
    {{#with formBlock}} 
      <section class="mb-3">
        <h1 class="text-dark">{{header}}</h1>
        <div>{{title}}: {{value}}</div>
        <div class="mb-5">
          <ul class="nav nav-tabs">
            {{#with actionBlock}}
              {{#each tabs}}
                <li class="nav-item">
                  <a class="nav-link {{#if isActive}} active {{/if}}" href="#">{{text}}</a>
                </li>
              {{/each}}
          </ul>
        </div>
        <hr>
              {{#with report}}
                <div class="mt-5 mb-5">
                  <div class="card">
                    <div class="card-body">
                      <h3>{{title}}</h3>
                      <a class="btn btn-secondary" href="{{button.link}}">{{button.text}}</a>
                    </div>
                  </div>
                </div>
              {{/with}}
            {{/with}}
        <hr>
              {{#with feedbackBlock}}
                <div class="mt-5 mb-5">
                  <h3>{{title}}</h3>
                  {{#each buttons}}                    
                    <a class="mt-3 mb-3 btn {{#if isWarning}} btn-warning
                    {{else}}
                      {{#if isSuccess}}
                        btn-success
                      {{else}}
                        btn-danger                        
                      {{/if}}
                    {{/if}}" href="{{link}}">{{text}}</a>
                  {{/each}}
                </div>
              {{/with}}
      </section>
    {{/with}}
  </div>
</div>
<hr>
<section>
  <div class="col-sm-10 row row-cols-1 row-cols-md-5 g-4">
  {{#each footer}}
    <div class="col-2">
      <ul class="nav">
        <li class="nav-item">                
          {{#each this}}
              <a href="{{link}}" class="nav-link {{#if @last}} disabled {{/if}}">{{text}}</a>
          {{/each}}
        </li>
      </ul>
    </div>
  {{/each}}
  </div>
</section>