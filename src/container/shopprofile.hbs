{{#with navigation}}  
<section class="mb-3">
  <div class="row card">
    <ul class="nav">
        {{#each links}}
      <li class="nav-item">
        <a class="nav-link {{#if @last}} disabled {{/if}}" href="{{href}}">{{text}}</a>
      </li>
        {{/each}}
    </ul>
  </div>
</section>
{{/with}}
  <section class="mb-3">
    <h1 class="text-dark">{{header}}</h1>
  </section>
{{#with purchase}}
<section class="mb-3">
  <div class="mb-3">
    <ul class="nav nav-tabs">
      {{#each tabs}}
        <li class="nav-item">
          <a class="nav-link {{#if @first}} active {{/if}}" href="#">{{this}}</a>
        </li>
      {{/each}}
    </ul>
    {{#with actionBlock}}
    <div class="d-flex mt-2">
      <input type="text" class="form-control w-25 me-2" placeholder="{{input.placeholder}}">
      {{#each buttons}}
        <a class="btn {{#if isSuccess}} btn-success {{else}}
        {{#if isOutline}}btn-outline-success {{else}} btn-outline-danger {{/if}}{{/if}}me-2" href="{{link}}">{{text}}</a>
      {{/each}}
    </div>
    {{/with}}
    <hr>
    {{#each itemList}}
      <div class="card mb-4">
        <div class="col-12">
          <div class="row ps-3">
            <div class="col-4 pt-2">
              <div class="fs-5">{{number}}</div>
              <div class="fs-4 fw-bold">{{status}}</div>
            </div>
            <div class="col-2 pt-2 d-flex flex-column">
              <div class="pt-2">{{amount.title}}</div>
              <div class="fs-4 fw-bold">{{amount.value}}</div>
            </div>
            <div class="col-4">
              {{#each images}}
                <img src="{{this}}" alt="#" class="me-2">
              {{/each}}
            </div>
            <div class="col-2 d-flex align-self-center">
              {{#with button}}
                <a href="{{link}}" class="btn {{#if isPrimary}} btn-primary {{/if}}">{{text}}</a>
              {{/with}}
            </div>
          </div>
        </div>
      </div>
    {{/each}}
    <hr>
  </div>
</section>
{{/with}}
<hr>
{{#with userInfo}}
  <section class="mb-3">
    <div class="fs-2 fw-bold mb-2">{{title}}</div>
    <div class="card">
      <div class="card-body">
        {{#with profileData}}
          <div class="fs-4 fw-bold mb-2">{{title}}</div>
          {{#with fullName}}
            <div class="card mb-2">
              <div class="card-body">
                <div class="row">
                  {{#with surname}}
                    <div class="col-4">
                      <div class="fs-6">{{title}}</div>
                      <div class="fs-6 fw-bold">{{value}}</div>
                    </div>
                  {{/with}}
                  {{#with name}}
                    <div class="col-4">
                      <div class="fs-6">{{title}}</div>
                      <div class="fs-6 fw-bold">{{value}}</div>
                    </div>
                  {{/with}}
                  {{#with middleName}}
                    <div class="col-4">
                      <div class="fs-6">{{title}}</div>
                      <div class="fs-6 fw-bold">{{value}}</div>
                    </div>
                  {{/with}}
                </div>
              </div>
            </div>
          {{/with}} <!-- end of fullname -->
          {{#with otherInfo}}
            <div class="card mb-2">
              <div class="card-body">
                <div class="row">
                  {{#with birthday}}
                    <div class="col-4">
                      <div class="fs-6">{{title}}</div>
                      <div class="fs-6 fw-bold">{{value}}</div>
                    </div>
                  {{/with}}
                  {{#with sex}}
                    <div class="col-4">
                      <div class="fs-6">{{title}}</div>
                      <div class="fs-6 fw-bold">{{value}}</div>
                    </div>
                  {{/with}}
                  {{#with language}}
                    <div class="col-4">
                      <div class="fs-6">{{title}}</div>
                      <div class="fs-6 fw-bold">{{value}}</div>
                    </div>
                  {{/with}}
                </div>
              </div>
            </div>
          {{/with}} <!-- end of otherInfo -->
        {{/with}} <!-- end of profileData -->
        <div class="card">
          <div class="card-body">
            <div class="row">
              {{#each buttons}}
                <div class="col-4">
                  <a href="{{link}}" class="btn {{#if isPrimary}} btn-primary {{else}}
                  {{#if isSecondary}} btn-outline-secondary {{else}} btn-outline-danger {{/if}}{{/if}}">{{text}}</a>
                </div>
              {{/each}} <!-- end of buttons -->
            </div>
          </div>
        </div>
        {{#with recipients}}
          <div class="fs-4 fw-bold mb-2">{{title}}</div>
          <div class="row">
            {{#each users}}
              <div class="col-4">
                <div class="card">
                  <div class="card-body">
                    <div class="fw-bold mb-2">{{name}}</div>
                    <div class="mb-2">{{phone}}</div>
                    <a href="{{button.link}}" class="btn btn-primary">{{button.text}}</a>
                  </div>
                </div>
              </div>
            {{/each}} <!-- end of users -->
          </div>
        {{/with}} <!-- end of recipients -->
        {{#with contacts}}
          <div class="fs-4 fw-bold mb-2">{{title}}</div>
          <div class="row">
            {{#each contactData}}
              <div class="col-4">
                <div class="card">
                  <div class="card-body">
                    <div class="mb-2">{{title}}</div>
                    <div class="fw-bold mb-2">{{value}}</div>
                    <a href="{{button.link}}" class="btn {{#if isPrimary}} btn-primary {{else}}
                    {{#if isSecondary}} btn-outline-secondary {{else}} btn-outline-danger {{/if}}{{/if}}">{{button.text}}</a>
                  </div>
                </div>
              </div>
            {{/each}} <!-- end of contactData -->
          </div>
        {{/with}} <!-- end of contacts -->
      </div>
    </div>
  </section>
{{/with}}
<hr>
<section>
  <div class="col-sm-10 row row-cols-1 row-cols-md-5 g-4">
  {{#each footer}}
    <div class="col-2">
      <ul class="nav">
        <li class="nav-item">                
          {{#each this}}
              <a href="{{link}}" class="nav-link {{#if @last}} disabled {{/if}}">{{text}}</a>
          {{/each}}
        </li>
      </ul>
    </div>
  {{/each}}
  </div>
</section>