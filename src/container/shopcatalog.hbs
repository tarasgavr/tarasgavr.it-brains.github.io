{{#with navigation}}  
  <section class="mb-3">
    <div class="row card">
      <ul class="nav">
          {{#each links}}
        <li class="nav-item">
          <a class="nav-link {{#if @last}} disabled {{/if}}" href="{{href}}">{{text}}</a>
        </li>
          {{/each}}
      </ul>
    </div>
  </section>
{{/with}}
<section>
  <nav>
    <ul class="breadcrumb">
      {{#each breadcrumb}}        
        <li class="breadcrumb-item">
          <a {{#if url}} href="{{url}}" {{/if}}>{{name}}</a>
        </li>
      {{/each}}
    </ul>
  </nav>
</section>
<form action="#">
  <h1 class="m-3">{{header}}</h1>
  <div class="cols-9 row row-cols-1 row-cols-md-3 g-2 mb-3 p-2">
    {{#each sortBlock}}
      <div class="col">
        <select class="form-select me-3" aria-label="Default select example">
          <option selected>{{placeholder}}</option>
          {{#each options}}
            <option value="{{value}}">{{text}}</option>
          {{/each}}
        </select>
      </div>
    {{/each}}
  </div>
  <div class="row row-cols-1 row-cols-md-3 g-2">
    <div class="col">
      <div class='card'>
        <div class='card-body'>
            {{#with filterBlock}}
              {{#with sellers}}
                {{title}}
                  {{#each list}} 
                    <input type="checkbox" id="mach-seller">
                    <label for="mach-seller">{{value}}</label>
                  {{/each}}
              {{/with}}
              <hr>
              {{#with brands}}
                {{title}}
                <input type="text" placeholder="{{input.placeholder}}">
                <div class="mt-2">
                  {{#each options}}
                    <input type="checkbox" id="mach-opt">
                    <label for="mach-opt">{{value}}</label>
                  {{/each}}
                </div>
              {{/with}}
              <hr>
              {{#with price}}
                {{title}}
                <input type="range" min="{{range.from}}" max="{{range.to}}" value="{{range.from}}">
                <input type="range" min="{{range.from}}" max="{{range.to}}" value="{{range.to}}">
                <a href="{{buttons.0.link}}" class="btn btn-primary">{{buttons.0.text}}</a>
              {{/with}}
            {{/with}}
        </div>
      </div>
    </div>
  </div>
</form>
<section>
  {{#with service}}
    <div class="w-100 bg-success text-center p-3">
      <h2 class="text-light mb-2">{{title}}</h2>
      <p class="text-light mb-2">{{description}}</p>
      <a href="{{buttons.0.link}}" class="btn btn-primary">{{buttons.0.text}}</a>
    </div>
  {{/with}}
</section>
<section>
  <div class="col-sm-10 row row-cols-1 row-cols-md-5 g-2">
  {{#each footer}}
    <div class="col-2">
      <ul class="nav">
        <li class="nav-item">                
          {{#each this}}
              <a href="{{link}}" class="nav-link {{#if @last}} disabled {{/if}}">{{text}}</a>
          {{/each}}
        </li>
      </ul>
    </div>
  {{/each}}
  </div>
</section>