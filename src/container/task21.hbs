{{#with heading}}  
<section class="card">
      {{#with main}}
    <div class="card">
      <div class="card-body">
        <h1 class="text-dark">{{title}}</h1>
        <dfn class="text-dark">{{text}}</dfn>
        <br>
        {{#with button_text}}
          <button type="button" class="btn btn-primary">{{solid}}</button>
          <br>
          <br>
          <button type="button" class="btn btn-outline-primary">{{outline}}</button>
        {{/with}}
      </div>
    </div>
    <br>
      {{/with}}
      {{#with second}}
    <div class="card">
      <div class="card-body">
        <h2 class="text-secondary">{{title}}</h2>
        <dfn class="text-dark">{{text}}</dfn>
        <br>
        {{#with button_text}}
          <button type="button" class="btn btn-secondary">{{solid}}</button>
          <br>
          <br>
          <button type="button" class="btn btn-outline-secondary">{{outline}}</button>
        {{/with}}
      </div>
    </div>
    <br>
      {{/with}}
      {{#with subblock}}
    <div class="card">
      <div class="card-body">
        <h3 class="text-dark">{{title}}</h3>
        <dfn class="text-dark">{{text}}</dfn>
        <br>
        {{#with button_text}}
          <button type="button" class="btn btn-dark">{{solid}}</button>
          <br>
          <br>
          <button type="button" class="btn btn-outline-dark">{{outline}}</button>
        {{/with}}
      </div>
    </div>
      {{/with}}
</section>
{{/with}}
<br><br><br>
{{#with nested_display}}
<section class="card">  
  <div class="card-body">
    <div class="display-1">{{title}}</div>
    <br>
      {{#with children_display}}
    <div class="card">
      <div class="card-body">
        <div class="display-4">{{title}}</div>
        <dfn class="text-dark">{{text}}</dfn>
        <br>
        <button type="button" class="btn btn-outline-warning">{{button.text}}</button>
      {{/with}}
      </div>
    </div>
    <h6 class="text-dark">{{header_text}}</h6>
    <button type="button" class="btn btn-info text-light">{{button.text}}</button>
  </div>
</section>
{{/with}}
<br><br><br>
{{#with paragraph}}
<section class="card">  
  <div class="card-body">
    <div class="text-uppercase">{{title}}</div>
    <dfn class="text-dark">{{text}}</dfn>
    <br>
      {{#with button}}
    <div class="text-info">{{> link href=url text=text noLine=true}}</div></div>
      {{/with}}
    <div class="card">
      <div class="card-body">
        <ul>
          {{#each list_name}}
          <li>
            <div class="text-info">{{> link href=url text=text noLine=true}}</div>
          </li>
          <br>
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</section>  
{{/with}}